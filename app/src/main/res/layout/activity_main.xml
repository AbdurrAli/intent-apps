<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginHorizontal="16dp"
    android:gravity="center"
    android:orientation="vertical"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/btn_change_activity"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/btn_move"
        android:paddingVertical="14dp" />

    <Button
        android:id="@+id/btn_change_activity_with_data"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingVertical="14dp"
        android:layout_marginTop="8dp"
        android:text="@string/btn_move_with_data" />

    <Button
        android:id="@+id/btn_change_activity_with_objek"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingVertical="14dp"
        android:layout_marginTop="8dp"
        android:text="@string/btn_move_with_obj" />

    <Button
        android:id="@+id/btn_dial_number"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingVertical="14dp"
        android:layout_marginTop="8dp"
        android:text="@string/btn_dial_number"/>

    <Button
        android:id="@+id/btn_move_with_result"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/btn_move_with_result"
        android:paddingVertical="14dp"
        android:layout_marginTop="8dp"
        />

    <TextView
        android:id="@+id/tv_data_result"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/label_result"
        android:textSize="16sp"
        android:layout_marginTop="8dp"
        android:textAlignment="center"
        />

</LinearLayout>